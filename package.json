  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Tuval Simha",
  "license": "ISC",
  "dependencies": {
    "@graphql-typed-document-node/core": "3.2.0",
    "graphql": "16.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "typescript": "5.4.5",
    "@parcel/watcher": "2.4.1"
  }
