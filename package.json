{
  "name": "create-graphql-react-app",
  "version": "1.0.2",
  "description": "A starter project for a fullstack GraphQL app with React, GraphQL Codegen, GraphQL Yoga and Vite.",
  "main": "index.js",
  "workspaces": [
    "backend",
    "client"
  ],
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts"
  },
